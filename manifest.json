{
  "manifest_version": 3,
  "name": "Daily Habits Tracker",
  "version": "1.0",
  "description": "Track your daily habits right from your new tab page",
  "chrome_url_overrides": {
    "newtab": "habits.html"
  },
  "permissions": [
    "storage"
  ],
  "host_permissions": [
    "http://localhost:5000/*"
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
} 